import { onDocumentWritten } from "firebase-functions/v2/firestore";
import { Change, FirestoreEvent } from "firebase-functions/v2/firestore";
import { DocumentSnapshot } from "firebase-admin/firestore";

exports.onUserUpdate = onDocumentWritten(
  "users/{userId}",
  (event: FirestoreEvent<Change<DocumentSnapshot> | undefined, { userId: string }>) => {
    
    // Get document ID from path parameters
    const userId = event.params.userId;
    
    // Access before/after snapshots
    const beforeData = event.data?.before.data();
    const afterData = event.data?.after.data();

    // Check if document was created, updated, or deleted
    if (!event.data?.before.exists && event.data?.after.exists) {
      console.log(`User ${userId} created:`, afterData);
    } else if (event.data?.before.exists && event.data?.after.exists) {
      console.log(`User ${userId} updated:`);
      console.log("Before:", beforeData);
      console.log("After:", afterData);
    } else if (event.data?.before.exists && !event.data?.after.exists) {
      console.log(`User ${userId} deleted:`, beforeData);
    }
  }
);
```

